{
  "manifest_version": 2,

  "name": "Chrome HSTS",
  "description": "Chrome HSTS",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": { 
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png" 
  },

  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
